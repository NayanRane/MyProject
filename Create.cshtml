            <input type="hidden" asp-for="Item1.ClaimNo" name="ClaimNo" value="1">
            <input type="hidden" asp-for="Item1.MileageStatus" name="MileageStatus" value="1">
            @* <input type="hidden" asp-for="Item1.CompanyCode" name="CompanyCode" value="C"> *@
            <input type="hidden" asp-for="Item1.Approver1Name" name="Approver1Name" id="Approver1Name">
            <input type="hidden" asp-for="Item1.Approver2Name" name="Approver2Name" id="Approver2Name">
            <input type="hidden" asp-for="Item1.EmployeeCompanyName" name="EmployeeCompanyName" value="C">
            <input type="hidden" asp-for="Item1.EmployeeCompanyCode" name="EmployeeCompanyCode" value="C">
            <input type="hidden" asp-for="Item1.EmployeeDepartmentName" name="EmployeeDepartmentName" value="D">
            <input type="hidden" asp-for="Item1.EmployeeDepartmentCode" name="EmployeeDepartmentCode" value="D">
            <input type="hidden" name="CompanyName" value="C">
