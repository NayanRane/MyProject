@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
    <script>
        var alertMessage = @(Html.Raw(JsonConvert.SerializeObject(ViewData["Alert"])));
        helpData = @(Html.Raw(JsonConvert.SerializeObject(ViewData["HelpPageDetails"])));
        let projects = @(Html.Raw(JsonConvert.SerializeObject(@ViewBag.Projects)));
        let MileageList = @(Html.Raw(JsonConvert.SerializeObject(ViewData["MileageList"])));
        let returnFromDate = @(Html.Raw(JsonConvert.SerializeObject(ViewData["CatchFromDate"])));
        let returnToDate = @(Html.Raw(JsonConvert.SerializeObject(ViewData["CatchToDate"])));
        @* let MileageRate = @(Html.Raw(JsonConvert.SerializeObject(ViewData["MileageRate"]))); *@
        var HoverPrompts = @(Html.Raw(JsonConvert.SerializeObject(ViewData["HoverPrompts"])));

        if (alertMessage != null) {
            let globArr = [];
            let data = alertMessage.split('|');
            data.forEach(function (obj) {
                globArr.push(obj);
            });
            let message = '';
            for (i = 0; i < globArr.length - 1; i++) {
                message += i + 1 + ". " + globArr[i].split(";").at(-1) + "\n";
            }
            if (message !== '' && message !== null) {
                Swal.fire({
                    title: message,
                    icon: "error",
                    buttonsStyling: false,
                    confirmButtonText: "Ok",
                    customClass: {
                        confirmButton: "btn btn-sm button-base"
                    }
                });
            }
        }
    </script>

    <script src="~/js/mileageclaims/create.js"></script>
    <script src="~/js/mileageclaims/map.js"></script>
    @* <script src="~/js/mileageclaims/displaymap.js"></script> *@

}